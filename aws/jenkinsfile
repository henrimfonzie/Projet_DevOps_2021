pipeline {
    agent any

    stages {
        stage('Test') {
            steps {
                sh 'python3 /app/test.py'
            }
        }
        stage('Build  clean & deploy via Ansible') {
            steps {
                sh 'ansible-playbook -i /home/ubuntu/machines.txt /home/ubuntu/Appli-playbook.yml'
            }
        }
    }
}